{
  "name": "vuex-peek",
  "displayName": "vuex-peek",
  "publisher": "suxq",
  "description": "vuex peek",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vuex.goto-store",
    "onCommand:vuex.update-store",
    "onLanguage:vue",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vuex.goto-store",
        "title": "Go to vuex store"
      },
      {
        "command": "vuex.update-store",
        "title": "Update vuex store"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && editorHasSelection && resourceLangId == vue",
          "command": "vuex.store",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
